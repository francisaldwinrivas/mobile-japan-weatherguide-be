{
    "preset": "laravel",
    "rules": {
        "declare_strict_types": true,
        "no_superfluous_phpdoc_tags": false,
        "phpdoc_no_empty_return": false,
        "phpdoc_trim": false,
        "general_phpdoc_annotation_remove": false,
        "phpdoc_separation": false,
        "phpdoc_align": false
    }
}